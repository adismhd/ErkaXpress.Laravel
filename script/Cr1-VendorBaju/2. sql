
CREATE TABLE `xkecamatans` (
  `id` bigint(20) AUTO_INCREMENT  NOT NULL,
  `Code` varchar(255) NOT NULL,
  `KabupatenId` varchar(255) NOT NULL,
  `Nama` varchar(255) NOT NULL,
  `IsActive` varchar(255) NOT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`id`)
);

CREATE TABLE `xkelurahans` (
  `id` bigint(20) AUTO_INCREMENT NOT NULL,
  `Code` varchar(255) NOT NULL,
  `KecamatanId` varchar(255) NOT NULL,
  `Nama` varchar(255) NOT NULL,
  `IsActive` varchar(255) NOT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`id`)
);

CREATE TABLE `xwilayahs`  (
  `id` bigint(20) AUTO_INCREMENT NOT NULL,
  `Code` varchar(255) NOT NULL,
  `Nama` varchar(255) NOT NULL,
  `IsActive` varchar(255) NOT NULL,
  `PropinsiId` varchar(255) NOT NULL,
  `KabupatenId` varchar(255) NOT NULL,
  `KecamatanId` varchar(255) NOT NULL,
  `KelurahanId` varchar(255) NOT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`id`)
);

CREATE TABLE `dokumens` (
  `id` bigint(20) AUTO_INCREMENT NOT NULL,
  `NoPesanan` varchar(255) NOT NULL,
  `KodeDokumen` varchar(255),
  `Blob` TEXT,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`id`)
);

